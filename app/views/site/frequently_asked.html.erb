<div class="row">
  <div class="span12">
    <div class="well">
      <div class="page-header"><h1>Frequently Asked Questions</h1></div>
      <% @faq_list = Faq.order(:order_index) %>
      <% @faq_list.each do |faq| %>
        <h3><%= faq.question %>?</h3>
        <p class="ccc-p"><%= faq.answer %></p>
      <% end %>        
      
    </div>
  </div>
</div>
